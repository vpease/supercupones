{
  "name": "pouchdb-gql",
  "version": "1.0.0",
  "description": "Google Query language plugin for pouchdb",
  "main": "pouchdb.gql.js",
  "scripts": {
    "test": "./node_modules/.bin/jshint -c .jshintrc *.js test/*.js && ./node_modules/istanbul/lib/cli.js test ./node_modules/mocha/bin/_mocha test/test.js",
    "build": "mkdir -p dist && browserify pouchdb.gql.js -o dist/pouchdb.gql.js -s GQL"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pouchdb/GQL.git"
  },
  "author": "",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/pouchdb/GQL/issues"
  },
  "homepage": "https://github.com/pouchdb/GQL",
  "dependencies": {
    "pouchdb-collate": "~0.1.1"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "mocha": "~1.16.2",
    "jshint": "~2.3.0",
    "pouchdb": "git://github.com/daleharvey/pouchdb.git#master",
    "istanbul": "~0.2.3",
    "underscore": "~1.5.2",
    "browserify": "~3.23.1"
  }
}
